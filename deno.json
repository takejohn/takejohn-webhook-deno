{
    "imports": {
        "hono": "jsr:@hono/hono@^4.6.17"
    },
    "fmt": {
        "indentWidth": 4,
        "useTabs": false,
        "singleQuote": true
    },
    "tasks": {
        "dev": "deno serve --watch main.ts",
        "start": "deno serve main.ts"
    },
    "compilerOptions": {
        "jsx": "precompile",
        "jsxImportSource": "hono/jsx"
    }
}
