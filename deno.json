{
    "imports": {
        "hono": "jsr:@hono/hono@^4.6.17",
        "misskey-js": "npm:misskey-js@^2025.1.0-beta.0"
    },
    "fmt": {
        "indentWidth": 4,
        "useTabs": false,
        "singleQuote": true
    },
    "tasks": {
        "dev": "deno serve --env --allow-env --watch src/main.ts",
        "start": "deno serve --env --allow-env src/main.ts"
    },
    "compilerOptions": {
        "jsx": "precompile",
        "jsxImportSource": "hono/jsx"
    }
}
